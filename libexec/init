#!/bin/sh

if [ -z "$HSENV_SH_ROOT" ]; then
  rel_root="`dirname $0`/.."
  export HSENV_SH_ROOT=`cd "$rel_root"; pwd`
fi

export _hsenv_private_libexec_dir="${HSENV_SH_ROOT}/libexec"
export _hsenv_private_template_dir="${HSENV_SH_ROOT}/template"

. "$_hsenv_private_libexec_dir/log"
. "$_hsenv_private_libexec_dir/host"
